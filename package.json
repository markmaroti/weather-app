{
  "name": "weather",
  "version": "16.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --ci --coverage",
    "lint": "ng lint && npm run lint:styles",
    "prepare": "git config core.hooksPath ./git-hooks || exit 0",
    "lint:styles": "stylelint --syntax=scss --config=./stylelint **/*.scss",
    "lint-staged": "lint-staged -r",
    "precommit": "npm run lint-staged"
  },
  "private": true,
  "lint-staged": {
    "*.ts": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "*.scss": "stylelint --syntax=scss --config=./stylelint"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@auth0/angular-jwt": "^5.0.2",
    "@deejayy/api-caller": "^16.0.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "immer": "^9.0.12",
    "modern-css-reset": "^1.4.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-preset-angular": "^13.1.6",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.1"
  }
}
