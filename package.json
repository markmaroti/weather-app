{
  "name": "weather",
  "version": "15.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --ci --coverage",
    "lint": "ng lint && npm run lint:styles",
    "prepare": "git config core.hooksPath ./git-hooks || exit 0",
    "lint:styles": "stylelint --syntax=scss --config=./stylelint **/*.scss",
    "lint-staged": "lint-staged -r",
    "precommit": "npm run lint-staged"
  },
  "private": true,
  "lint-staged": {
    "*.ts": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "*.scss": "stylelint --syntax=scss --config=./stylelint"
  },
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@auth0/angular-jwt": "^5.0.2",
    "@deejayy/api-caller": "^15.0.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "immer": "^9.0.12",
    "modern-css-reset": "^1.4.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.3.1",
    "jest-html-reporter": "^3.4.1",
    "jest-preset-angular": "^11.1.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.8.4"
  }
}
